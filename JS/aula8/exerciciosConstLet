// Luiz Otávio 30 anos, pesa 84KG
//tem 1.8 de altura e seu IMC é de 25.9259259


const nome = 'Luiz Otávio';
const sobrenome = 'Miranda'
const nomeCompleto = nome + ' '+ sobrenome;
const peso = 84;
const alturaM = 1.8;
const idade = 30;
let imc = peso/(alturaM*alturaM);
let anoNascimento = 2019- idade;

console.log(`${nomeCompleto} tem ${idade}, pesa ${peso} KG, tem ${alturaM} e seu IMC é de ${imc}`);
console.log(`${nome} nasceu em ${anoNascimento}`);


